<h1 class="announcement_new_edit_header">New announcement</h1>
<%= form_for(@announcement) do |f| %>
    <%= render 'shared/error_messages', object: @announcement %>
    <%= f.text_area :content_unevaluated, rows: 4 %>
    <%= f.submit "Add Announcement", class: "btn btn-large btn-custom" %>
<% end %>