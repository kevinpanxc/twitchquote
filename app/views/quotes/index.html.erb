<% provide(:title, 'Quotes from Twitch Chat') %>

<div class="row">
    <div class="col-md-10">
        <ul class="quotes">
            <%= render @quotes %>
        </ul>
    </div>
    <div class="col-md-2 hidden-xs hidden-sm">
        <div style="margin-bottom: 25px">
            <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
            <!-- TwitchQuotes Main Page -->
            <ins class="adsbygoogle"
                 style="display:inline-block;width:120px;height:600px"
                 data-ad-client="ca-pub-0144173183236578"
                 data-ad-slot="6799625657"></ins>
            <script>
            (adsbygoogle = window.adsbygoogle || []).push({});
            </script>
        </div>
        <div>
            <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
            <!-- TwitchQuotes Main Page 2 -->
            <ins class="adsbygoogle"
                 style="display:inline-block;width:120px;height:600px"
                 data-ad-client="ca-pub-0144173183236578"
                 data-ad-slot="8136758056"></ins>
            <script>
            (adsbygoogle = window.adsbygoogle || []).push({});
            </script>
        </div>
    </div>
</div>

<div class="paginate_parent_bottom"><%= will_paginate %></div>